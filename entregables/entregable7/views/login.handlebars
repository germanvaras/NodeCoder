<div id="loginContainer">
    <form class="formContainer" id="formLogin">
        <h2 class="formTitle">Ingresá</h2>
        <input type="text" placeholder="Email o Username" name="userEmail" class="formInput" id="userEmail" required />
        <input type="text" placeholder="Contraseña" name="userPassword" class="formInput" id="userPassword" required
            maxlength="20" />
        <button class="buttonForm" type="submit">Ingresar</button>
        <a class="linkForm" href="auth/github">Iniciar sesión con GitHub </a>
        <p>No tienes una cuenta? <a class="linkForm" href="/api/user/register">Registrate</a></p>
    </form>
</div>
<script src="/js/login.js"></script>
